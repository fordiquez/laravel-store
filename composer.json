{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bensampo/laravel-enum": "^6.3",
        "bezhansalleh/filament-shield": "^3.0@beta",
        "filament/filament": "^3.0-stable",
        "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "laravel/cashier": "^14.12",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.13",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.12",
        "laravel/tinker": "^2.7",
        "mohammadhprp/filament-ip-to-country-flag-column": "^1.0",
        "opcodesio/log-viewer": "^3.0",
        "phpsa/filament-password-reveal": "^2.0.0",
        "predis/predis": "^2.1",
        "tightenco/ziggy": "^1.0",
        "ysfkaya/filament-phone-input": "^2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.18",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
